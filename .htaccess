RewriteEngine On

RewriteRule ^([^/]*)/?$ /project_services/config/routes.php?req=$1 [L,QSA]

RewriteRule ^([^/]*)/([a-z]+)/?$ /project_services/config/routes.php?req=$1&opt=$2 [L,QSA]

RewriteRule ^([^/]*)/([a-z]+)/([0-9]+)/?$ /project_services/config/routes.php?req=$1&opt=$2&val=$3 [L,QSA]

RewriteRule ^([^/]*)/key-([0-9]+)/?$ /project_services/config/routes.php?req=$1&key=$2&page= [L,QSA]

RewriteRule ^([^/]*)/key-([0-9]+)/([a-z]+)/?$ /project_services/config/routes.php?req=$1&key=$2&opt=$3 [L,QSA]

RewriteRule ^([^/]*)/key-([0-9]+)/([a-z]+)/([0-9]+)/?$ /project_services/config/routes.php?req=$1&key=$2&opt=$3&val=$4 [L,QSA]

RewriteRule ^([^/]*)/key-([0-9]+)/([a-z]+)/([0-9]+)/?$ /project_services/config/routes.php?req=$1&key=$2&opt=$3&val=$4 [L,QSA]

RewriteRule ^([^/]*)/key-([0-9]+)/([0-9]+)/?$ /project_services/config/routes.php?req=$1&key=$2&page=$3 [L,QSA]

RewriteRule ^([^/]*)/([a-z]+)/([0-9]+)/([0-9]+)/?$ /project_services/config/routes.php?req=$1&opt=$2&val=$3&page=$4 [L,QSA]
